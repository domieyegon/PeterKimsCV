<script>
    window.sr = ScrollReveal();
    sr.reveal('.gal-top',{
        duration: 2000,
        origin: 'top',
        distance: '200px'
    });
    sr.reveal('.txtgal',{
        duration: 2000,
        origin: 'bottom',
        distance: '30px'
    });
    sr.reveal('.gal-left',{
        duration: 2000,
        origin: 'left',
        distance: '200px'
    });
    sr.reveal('.gal-right',{
        duration: 2000,
        origin: 'right',
        distance: '200px'
    });
    sr.reveal('.gal-center',{
        duration: 2000,
        delay: 1000,
        origin: 'bottom'
    });
</script>

